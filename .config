
#
# Platform
#
# CONFIG_PLAT_MT7622 is not set
# CONFIG_PLAT_MT7629 is not set
CONFIG_PLAT_MT7981=y
# CONFIG_PLAT_MT7986 is not set
# CONFIG_FPGA is not set
# end of Platform

#
# Target
#
# CONFIG_TARGET_BL2 is not set
# CONFIG_TARGET_BL31 is not set
CONFIG_TARGET_ALL_NO_SEC_BOOT=y
# CONFIG_NEED_SBC is not set
# CONFIG_FLASH_DEVICE_SNFI_SNAND is not set
# CONFIG_FLASH_DEVICE_SPIM_NAND is not set
CONFIG_FLASH_DEVICE_SPIM_NOR=y
# CONFIG_FLASH_DEVICE_EMMC is not set
# CONFIG_FLASH_DEVICE_SDMMC is not set
# CONFIG_FLASH_DEVICE_RAM is not set
# end of Target

#
# DRAM
#
CONFIG_DRAM_DDR3=y
# CONFIG_DRAM_DDR4 is not set
# CONFIG_QFN is not set
CONFIG_BGA=y
CONFIG_DRAM_SIZE_AUTO=y
# CONFIG_DRAM_SIZE_256 is not set
# CONFIG_DRAM_SIZE_512 is not set
CONFIG_DDR3_FREQ_2133=y
# CONFIG_DDR3_FREQ_1866 is not set
# end of DRAM

#
# Log
#
# CONFIG_LOG_LEVEL_NONE is not set
# CONFIG_LOG_LEVEL_ERROR is not set
CONFIG_LOG_LEVEL_NOTICE=y
# CONFIG_LOG_LEVEL_WARNING is not set
# CONFIG_LOG_LEVEL_INFO is not set
# CONFIG_LOG_LEVEL_VERBOSE is not set
# CONFIG_DRAM_DEBUG_LOG is not set
# end of Log

CONFIG_PLAT="mt7981"
CONFIG_AARCH64=y
CONFIG_NEED_BL2=y
CONFIG_NEED_BL31=y
CONFIG_NEED_BL33=y
CONFIG_NEED_FIP=y
CONFIG_BL33="../u-boot/u-boot.bin"
CONFIG_CROSS_COMPILER="/usr/bin/aarch64-linux-gnu-"
CONFIG_MTK_SPIM_SUPPORT=y
CONFIG_MTK_SNFI_SUPPORT=y
CONFIG_MTK_SDMMC_SUPPORT=y
CONFIG_BOOT_DEVICE="nor"
